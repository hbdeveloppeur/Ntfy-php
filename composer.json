{
    "name": "x00/ntfy-php",
    "description": "Send notifications to your phone in one line via Ntfy.sh for Symfony",
    "type": "composer-plugin",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "Ntfy\\": "src/"
        }
    },
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "composer-plugin-api": "^2.0",
        "symfony/http-kernel": "^5.4 || ^6.0 || ^7.0",
        "symfony/dependency-injection": "^5.4 || ^6.0 || ^7.0",
        "symfony/config": "^5.4 || ^6.0 || ^7.0",
        "symfony/yaml": "^5.4 || ^6.0 || ^7.0"
    },
    "extra": {
        "class": "Ntfy\\Composer\\InstallerPlugin"
    }
}